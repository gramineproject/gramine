{% set entrypoint = "Thread2" -%}

loader.entrypoint.uri = "file:{{ binary_dir }}/{{ entrypoint }}"

sgx.max_threads = 1
sgx.debug = true
sgx.edmm_enable = {{ 'true' if env.get('EDMM', '0') == '1' else 'false' }}
sgx.experimental_enable_aex_notify = {{ 'true' if env.get('AEXNOTIFY', '0') == '1' else 'false' }}
