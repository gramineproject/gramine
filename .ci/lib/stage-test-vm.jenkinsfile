stage('test') {
    timeout(time: 15, unit: 'MINUTES') {
        sh '''
            cd device-testing-tools/initramfs_builder

            ./run.sh PWD_FOR_VM=$PWD SGX=$SGX IS_VM=$IS_VM PATH="$PATH" \
                PKG_CONFIG_PATH="$PKG_CONFIG_PATH" PYTHONPATH="$PYTHONPATH" | tee OUTPUT
            grep "TESTS OK" OUTPUT
        '''
    }
}
